{
  "name": "@technicapital/cache-base",
  "description": "Basic object cache with `get`, `set`, `del`, and `has` methods for node.js/javascript projects.",
  "version": "4.1.0",
  "homepage": "https://github.com/technicapital/cache-base",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "(https://github.com/wtgtybhertgeghgtwtg)",
    "(https://github.com/technicapital)"
  ],
  "repository": "technicapital/cache-base",
  "bugs": {
    "url": "https://github.com/technicapital/cache-base/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "nyc --reporter=text --reporter=html mocha"
  },
  "dependencies": {
    "@sellside/emitter": "1.2.1",
    "collection-visit": "1.0.0",
    "get-value": "3.0.1",
    "has-own-deep": "1.1.0",
    "kind-of": "6.0.3",
    "lodash": "4.17.21",
    "set-value": "3.0.2",
    "union-value": "2.0.1",
    "unset-value": "1.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "2.0.0",
    "mocha": "8.2.1",
    "nyc": "15.1.0"
  },
  "keywords": [
    "base",
    "cache",
    "config",
    "data",
    "get",
    "has",
    "hash",
    "hasown",
    "object",
    "set",
    "store"
  ],
  "verb": {
    "run": true,
    "toc": true,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highligh": "base",
      "list": [
        "base-methods",
        "get-value",
        "has-value",
        "option-cache",
        "set-value",
        "unset-value"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
